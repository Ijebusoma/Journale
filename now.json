{
    "version": 2,
    "name": "my-diary",
    "public": false,
    "builds": [
      { "src": "client/package.json", "use": "@now/static-build" },
      { "src": "/server/server.js", "use": "@now/node-server" }
     
     
    ],
    "routes": [
      { "src": "/api/.*", "dest": "/server/server.js" },
      { "src": "^/static/(.*)", "dest": "client/dist" },
      { "src": ".*", "dest": "client/public/index.html" }
    ]
  }