{
  "name": "my-diary",
  "version": "1.0.0",
  "description": "My Diary is an online journal where users can pen down their thoughts and feelings.",
  "main": "app.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/bin/www",
    "test": "cross-env NODE_ENV=test mocha server/test/*.js --exit  --require babel-register --reporter spec",
    "start": "babel-node server/bin/www",
    "build": "babel ./server/ -d dist/server",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ijebusoma/My-Diary.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "author": "Vanessa Osuka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ijebusoma/My-Diary/issues"
  },
  "engines": {
    "node": "7.10.1"
  },
  "homepage": "https://github.com/Ijebusoma/My-Diary#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "nodemon": "^1.18.2"
  }
}
